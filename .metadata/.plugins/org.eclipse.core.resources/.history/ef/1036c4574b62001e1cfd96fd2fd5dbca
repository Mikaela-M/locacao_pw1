package util;
import java.time.LocalDate;
import java.util.*;

public interface Validador {
	
	public default boolean validarData(Date data1, Date data2) {
		if (data2.before(data1)) {
			return false;
		}
		if (data1.after(data2)) {
			return false;
		}
		//Ver outras validacoes
		return true;
	}
	
}
