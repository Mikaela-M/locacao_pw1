package util;

public enum Situacao {
	EMDIA(1, "Em dia"),
	CANCELADA(2, "Cancelada"),
	ATRASADA(3, "Atrasada")
	
	private String descricao;
	private int id;
	
	private Situacao(String descricao, int id) {
		this.descricao = descricao;
		this.id = id;
	}
	
	public String getDescricao() {
		return descricao;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}
	
	
}
